{
	"extends": "airbnb-base",
	"plugins": [
		"import"
	],
	"rules": {
		"arrow-parens": [
			"error",
			"always"
		],
		"padded-blocks": [
			"error",
			{
				"classes": "always"
			}
		],
		"class-methods-use-this": 0,
		"no-param-reassign": 0,
		"no-tabs": 0,
		"max-len": 0,
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ]
	}
}
